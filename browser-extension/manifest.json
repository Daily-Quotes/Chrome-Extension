{
  "name": "QuotesTab",
  "description": "Daily quotes!",
  "author": "Jose Naime",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["Background/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "Popup/popup.html"
  }
}